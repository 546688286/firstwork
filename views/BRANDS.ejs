						</li>
						<li>
							<a href="http://localhost:8081/BRANDS">BRANDS</a>
						</li>
						<li>
							<a href="http://localhost:8081/STYLISH">STYLISH</a>
						</li>
						<li>
							<a href="http://localhost:8081/CONTACT">CONTACT</a>
						</li>
					</ul>

				</div>
			</div>
		</div>
		<div class="content">
			<div class="wrap">
				<div class="row">
					<% for(var i=0;i<ban.length;i++){ %>
					<div class="img">
						<img src="<%= ban[i].src %>">
						<a>
							<p>
								<%= ban[i].text %>
							</p>
						</a>
						<div class="posiimg"></div>
					</div>
					<% } %>
				</div>
			</div>
			<div class="wqbtext">
				<div class="wqbtext_wqb">
					<b>ITEMS</b>
				</div>
			</div>
			<div class="cont">
				<div class="contcon">
					<% for(var i=0;i<bann.length;i++){ %>
						<div class="cc_cont">
							<img src="<%= bann[i].img %>">
							<p><%= bann[i].text %></p>
							<span><%= bann[i].type %></span>
							<span><%= bann[i].type2 %></span>
